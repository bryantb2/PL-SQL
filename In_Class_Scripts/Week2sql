-- SELECTS
SELECT * FROM SECTION;
SELECT * FROM ENROLLMENT;

SELECT COUNT(ENROLLMENT.FK_STUDENT_ID) AS STUDENTS_ENROLLED
  FROM SECTION
  JOIN ENROLLMENT ON SECTION.SECTION_ID = ENROLLMENT.FK_SECTION_ID
  WHERE SECTION.FK_CLASS_NUMBER = 'CS276' AND SECTION.SECTION_ID = 44172;

-- PROBLEM 1
set feedback on;
set serveroutput on;

DECLARE
  V_STUDENT_COUNT NUMBER;
BEGIN
  -- GET STUDENT COUNT
  SELECT COUNT(ENROLLMENT.FK_STUDENT_ID) AS STUDENTS_ENROLLED
  INTO V_STUDENT_COUNT
    FROM SECTION
    JOIN ENROLLMENT ON SECTION.SECTION_ID = ENROLLMENT.FK_SECTION_ID
    WHERE SECTION.FK_CLASS_NUMBER = 'CS276' AND SECTION.SECTION_ID = 44172;
    
  -- PROCESS COUNT
  DBMS_OUTPUT.PUT_LINE('CS276 HAS ' || V_STUDENT_COUNT || ' STUDENTS ENROLLED.');
  IF V_STUDENT_COUNT > 15
    THEN DBMS_OUTPUT.PUT_LINE('A WAITING LIST HAS BEEN STARTED FOR THIS CLASS.');
  ELSIF V_STUDENT_COUNT < 7
    THEN DBMS_OUTPUT.PUT_LINE('THIS CLASS NEEDS TO BE CANCELED :(');
  ELSIF V_STUDENT_COUNT >= 7 AND V_STUDENT_COUNT <= 15
    THEN DBMS_OUTPUT.PUT_LINE('THIS CLASS CAN CONTINUE!'); 
  END IF;  
END;


-- PROBLEM 2
set feedback on;
set serveroutput on;

DECLARE
  OPERATION VARCHAR2(1) := '*';
  FIRST_NUMBER NUMBER := 150;
  SECOND_NUMBER NUMBER := 2;
  ANSWER NUMBER := 0;
BEGIN
-- DO CALC
  CASE OPERATION
    WHEN '*' THEN
      ANSWER := FIRST_NUMBER * SECOND_NUMBER;
    WHEN '/' THEN
      ANSWER := FIRST_NUMBER / SECOND_NUMBER;
    WHEN '+' THEN
      ANSWER := FIRST_NUMBER + SECOND_NUMBER;
    WHEN '-' THEN
      ANSWER := FIRST_NUMBER - SECOND_NUMBER;
    ELSE
      DBMS_OUTPUT.PUT_LINE('OPERATION WAS NOT FOUND');
  END CASE;
  
  -- PRINT ANSWER
  DBMS_OUTPUT.PUT_LINE(FIRST_NUMBER || ' ' || OPERATION || ' ' || SECOND_NUMBER || ' = ' || ANSWER);
END;
















